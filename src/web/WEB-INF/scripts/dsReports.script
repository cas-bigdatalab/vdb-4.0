CREATE TABLE VDB_DQ_DATASET(DQID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DSID VARCHAR(255) NOT NULL, BYTES BIGINT DEFAULT 0, RECORDNUM BIGINT DEFAULT 0,ENTITYNUM INTEGER DEFAULT 0,RELATIONNUM INTEGER DEFAULT 0,RELATIONRATE DOUBLE,FIELDNUM INTEGER DEFAULT 0,TEXTFIELDNUM INTEGER DEFAULT 0,INTEGRITYRATE DOUBLE, LASTSTATSTIME VARCHAR(20))
CREATE TABLE VDB_DQ_REPOSITORY(RID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DSID VARCHAR(255) NOT NULL, DSURI VARCHAR(255), REPOSITORYNAME VARCHAR(100), DIRECTORYNUM BIGINT, MAXFILEBYTES BIGINT, MINFILEBYTES BIGINT, MAXDIRDEPTH BIGINT, LASTSTATSTIME VARCHAR(20))
CREATE TABLE VDB_DQ_FILE(FID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,RID INTEGER,CONTENTTYPE VARCHAR(100) DEFAULT '', FILEEXTENSION VARCHAR(20) DEFAULT '', BYTES BIGINT DEFAULT 0,ITEMS BIGINT DEFAULT 0,LASTSTATSTIME VARCHAR(20))
CREATE TABLE VDB_DQ_TABLE(DTID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DQID INTEGER, ENTITYID VARCHAR(255),ENTITYURI VARCHAR(255) ,TITLE VARCHAR(100) DEFAULT '',TABLENAME VARCHAR(100) DEFAULT '', BYTES BIGINT DEFAULT 0,RECORDNUM INTEGER DEFAULT 0,INTEGRITYRATE DECIMAL(20,2),LASTSTATSTIME VARCHAR(20))
CREATE TABLE VDB_DQ_FIELD(DFID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DTID INTEGER, FIELDID VARCHAR(255),NAME VARCHAR(100),TITLE VARCHAR(100),INTEGRITYRATE DECIMAL(20,2),LASTSTATSTIME VARCHAR(20))
CREATE TABLE KEY_WORDS_STATS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,KEY_WORD VARCHAR(255),DATASET VARCHAR(100),ENTITY VARCHAR(100),OP_COUNTS INTEGER DEFAULT 0,BY_DATE DATE NOT NULL)
CREATE TABLE DAILYSTATS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY, USER_NAME VARCHAR(255) NOT NULL,IP VARCHAR(15) NOT NULL,OP_DATE VARCHAR(20) NOT NULL,OPERATION VARCHAR(30) NOT NULL,OP_COUNTS INTEGER DEFAULT 0,DATASET VARCHAR(255),ENTITY VARCHAR(255),COUNTRY VARCHAR(50),AREA VARCHAR(200))